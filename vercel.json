{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "navy": true,
      "main": false
    }
  },
  "env": {
    "LOG_LEVEL": "INFO",
    "LOG_FILE_ENABLED": "false",
    "DATA_DIR": "/tmp/data",
    "SERVER_STORAGE_TYPE": "local",
    "SERVER_STORAGE_URL": ""
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    }
  }
}
