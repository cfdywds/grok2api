{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "fastapi",
  "buildCommand": "pip install --break-system-packages -r requirements.txt || uv pip install -r requirements.txt",
  "installCommand": "pip install --break-system-packages --upgrade pip || echo 'Using uv'",
  "git": {
    "deploymentEnabled": {
      "navy": true,
      "main": false
    }
  },
  "env": {
    "LOG_LEVEL": "INFO",
    "LOG_FILE_ENABLED": "false",
    "DATA_DIR": "/tmp/data",
    "SERVER_STORAGE_TYPE": "local",
    "SERVER_STORAGE_URL": ""
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    }
  }
}
